{
  "name": "next-tpl",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "deploy:vercel": "yarn qualityCheck && next build",
    "qualityCheck": "run-s qualityCheck:*",
    "qualityCheck:type": "tsc --project ./tsconfig.json --noEmit",
    "qualityCheck:unitTest": "jest --testPathIgnorePatterns .gitignore",
    "qualityCheck:redundancy": "jscpd .",
    "qualityCheck:findDeadCode": "ts-prune -e",
    "qualityCheck:esLint": "eslint --ignore-path .gitignore .",
    "qualityCheck:fmt": "prettier  --check --ignore-path .gitignore .",
    "qualityCheck:stylesLint": "stylelint --ignore-path .gitignore '**/styles.ts'",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@types/randomcolor": "0.5.6",
    "next": "12.1.4",
    "randomcolor": "0.6.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-syntax-highlighter": "15.5.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@bird-studio/eslint-config": "1.2.4",
    "@bird-studio/interactive-message": "2.0.0",
    "@bird-studio/js-config": "2.1.3",
    "@bird-studio/stylelint-config": "1.1.4",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.4",
    "@testing-library/react": "13.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.4.1",
    "@types/react": "17.0.43",
    "@types/react-syntax-highlighter": "13.5.2",
    "@types/styled-components": "5.1.24",
    "babel-plugin-jsx-remove-data-test-id": "3.0.0",
    "babel-plugin-styled-components": "2.0.6",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "jest": "27.5.1",
    "jscpd": "3.4.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2",
    "stylelint": "14.6.1",
    "ts-jest": "27.1.4",
    "ts-prune": "0.10.3",
    "typescript": "4.6.3"
  }
}
